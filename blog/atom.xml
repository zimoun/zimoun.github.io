---
layout: nil
---
<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">
 
 <title>another blog</title>
 <link href="http://zimoun.bitbucket.org/blog/atom.xml" rel="self"/>
 <link href="http://zimoun.bitbucket.org/blog/"/>
 <updated>{{ site.time | date_to_xmlschema }}</updated>
 <id>http://zimoun.bitbucket.org/blog/</id>
 <author>
   <name>Simon Tournier</name>
   <email>zimon.toutoune@gmail.com</email>
 </author>
 
 {% for post in site.categories.blog limit:5 %}
 <entry>
   <title>{{ post.title }}</title>
   <link href="http://zimoun.bitbucket.org{{ post.url }}"/>
   <updated>{{ post.date | date_to_xmlschema }}</updated>
   <id>id:{{ post.id }}</id>
   <content type="html">{{ post.content | xml_escape }}</content>
 </entry>
 {% endfor %}
 
</feed>